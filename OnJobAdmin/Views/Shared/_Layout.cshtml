<!DOCTYPE html>
<html>
<head>
    <title>Dashboard</title>
    <link rel="shortcut icon" href="https://codeglamour.com/php/onjob/assets/img/7066c9a97322b80b4288c250e9c34545.png">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" />
    
    <link rel="stylesheet" href="~/Content/assets/bootstrap/css/bootstrap.min.css">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">

    <link rel="stylesheet" href="~/Content/assets/dist/css/AdminLTE.min.css">
    <!-- Datatable style -->
    <link rel="stylesheet" href="~/Content/assets/plugins/datatables/dataTables.bootstrap.css">
    <!-- Custom CSS -->
    <link rel="stylesheet" href="~/Content/assets/dist/css/style.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Content/assets/dist/css/skins/skin-blue.min.css">
    <!-- jQuery 2.2.3 -->
    <script src="~/Content/assets/plugins/jQuery/jquery-2.2.3.min.js"></script>
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper" style="height: auto;">
        <section id="container">
            <!--header start-->
            <header class="header white-bg">
                @Html.Partial("_navbar")
            </header>
            <!--header end-->
            <!--sidebar start-->
            <aside>
                @Html.Partial("_admin_sidebar")
            </aside>
            <!--sidebar end-->
            <!--main content start-->
            <section id="main-content">
                <div class="content-wrapper" style="min-height: 394px; padding:15px;">
                    <!-- page start-->
                    @RenderBody()
                    <!-- page end-->
                </div>
            </section>
            <!--main content end-->
            <!--footer start-->
            <footer class="main-footer">
                <strong>Copyright © 2021 <a href="#">OnJOb Portal</a></strong> All rights reserved.<b>(Only Education Purpose)</b>
            </footer>
            <!--footer end-->
        </section>
    </div>

    <!-- jQuery UI 1.11.4 -->
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>
        $.widget.bridge('uibutton', $.ui.button);
    </script>
    <!-- Bootstrap 3.3.6 -->
    <script src="~/Content/assets/bootstrap/js/bootstrap.min.js"></script>

    <!-- AdminLTE App -->
    <script src="~/Content/assets/dist/js/app.min.js"></script>

    <!-- Date Picker -->
    <script src="~/Content/assets/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js"></script>

    <script type="text/javascript">
        $('.hr_datepicker').datepicker({ dateFormat: 'YY-mm-dd' });
    </script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

    <link rel="stylesheet" href="~/Content/assets/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Content/assets/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" href="~/Content/assets/plugins/datatables-buttons/css/buttons.bootstrap4.min.css">

    <script src="~/Content/assets/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Content/assets/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/Content/assets/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Content/assets/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <script src="~/Content/assets/plugins/datatables-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/Content/assets/plugins/datatables-buttons/js/buttons.bootstrap4.min.js"></script>



    <!-- page script -->
   

</body>
</html>